@model Real_Estate.Models.PropertyTypes

@{
    ViewData["Title"] = "Edit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<head>
    <!-- Design by foolishdeveloper.com -->
    <title>Glassmorphism login Form Tutorial in html css</title>

    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css">
    <link href="https://fonts.googleapis.com/css2?family=Poppins:wght@300;500;600&display=swap" rel="stylesheet">
    @*    <link href="~/css/site.css" rel="stylesheet" />*@
    <!--Stylesheet-->

</head>

<body class="login-register-Body">
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <h3 class="whiteText">Edit Property Type</h3>

            <label for="username" class="whiteText">Name</label>
            <input asp-for="Name" id="form3Example3cg" class="form-control form-control-lg" placeholder="Email" />
            <span asp-validation-for="Name" class="whiteText"></span>


            <label for="password" class="whiteText">Description</label>
            <input asp-for="Description" id="form3Example4cg" class="form-control form-control-lg" placeholder="Password" />
            <span asp-validation-for="Description" class="whiteText"></span>
            <div class="buttonDiv">
                <button class="whiteText">Save</button>
            </div>
            <div class="buttonDiv">
                <a asp-action="Index" class="whiteText">Back to List</a>
            </div>
        </form>
    </div>
</div>
</body>



@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
