<body class="login-register-Body">
    <div class="row">
        <div class="col-md-4">
            <form>
                <h3 class="whiteText">Details of Appointment</h3>

                <label for="username" class="whiteText">Property</label>
                <h2 class="whiteText">@Html.DisplayFor(model => model.Property.Name)</h2>

                <label for="password" class="whiteText">Client:</label>
                <h2 class="whiteText">@Html.DisplayFor(model => model.Clients.Name)</h2>

                <label for="password" class="whiteText">Owner:</label>
                <h2 class="whiteText">@Html.DisplayFor(model => model.Owners.Name)</h2>

                <label for="password" class="whiteText">Property Type:</label>
                <h2 class="whiteText">@Html.DisplayFor(model => model.PropertyTypes.Name)</h2>

                <label for="password" class="whiteText">Status:</label>
                <h2 class="whiteText">@Html.DisplayFor(model => model.Status)</h2>

                <div class="buttonDiv">
                    <button asp-action="Edit" asp-route-id="@Model?.Id" class="whiteText">Edit</button>
                    <div>
                        <a asp-action="Index" class="whiteText">Back to List</a>
                    </div>
                </div>
            </form>
        </div>
    </div>
</body>
